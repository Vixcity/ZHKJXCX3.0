<!--pages/quotedPrice/quotedPriceCreate.wxml-->
<view id="quotedPriceCreate">
	<nav-content showHome="{{false}}">
		<view slot="after">
			添加报价单
		</view>
	</nav-content>
	<view class="card">
		<l-input label="报价单标题" placeholder="点击输入" value="{{title}}" bind:lininput="changeTitle" />
		<view class="flex aic por fz28 color3">
			<text class="flex aic pl25r pr15r borderBox" style="width: 114px;justify-content: flex-start;height: 88rpx;">
				<text style="color: #e23;vertical-align: middle;">* </text>询价公司
			</text>
			<t-dropdown-menu z-index="2">
				<t-dropdown-item label="公司" optionsLayout="tree" options="{{clientList.options}}" value="{{clientList.value}}"
					bindchange="changeClient" />
			</t-dropdown-menu>
			<view class="poa bgColorF3" style="bottom: 0;right: 0;height: 2rpx;width: 730rpx;"></view>
		</view>
		<view class="flex aic por fz28 color3">
			<text class="flex aic pl25r pr15r borderBox" style="width: 114px;justify-content: flex-start;height: 88rpx;">
				<text style="color: #e23;vertical-align: middle;"></text>联系人
			</text>
			<view wx:if="{{!clientList.value[2] && !concatName}}" class="color91">请先选择询价公司</view>
			<view wx:if="{{clientList.value[2] && !concatName}}" bindtap="changeChooseConcat">选择联系人</view>
			<view wx:if="{{concatName}}" bindtap="changeChooseConcat">{{concatName.value}}</view>
			<van-popup show="{{ showChooseConcat }}" position="bottom" custom-style="height: 40%;" round>
				<van-picker columns="{{ concatNameList }}" show-toolbar title="选择联系人" bind:cancel="closeChooseConcat"
					bind:confirm="confirmChooseConcat" />
			</van-popup>
			<view class="poa bgColorF3" style="bottom: 0;right: 0;height: 2rpx;width: 730rpx;"></view>
		</view>
		<view class="flex aic por fz28 color3">
			<text class="flex aic pl25r pr15r borderBox" style="width: 114px;justify-content: flex-start;height: 88rpx;">
				<text style="color: #e23;vertical-align: middle;"></text>负责组/人
			</text>
			<view wx:if="{{!groupName}}" class="color91" bindtap="openGroup">点击选择</view>
			<view wx:else bindtap="openGroup">{{ groupName.value }}</view>
			<van-popup show="{{ showGroup }}" position="bottom" custom-style="height: 40%;" round>
				<van-picker columns="{{ groupLabelList }}" show-toolbar title="选择负责组/人" bind:cancel="closeGroup"
					bind:confirm="confirmGroup" />
			</van-popup>
			<view class="poa bgColorF3" style="bottom: 0;right: 0;height: 2rpx;width: 730rpx;"></view>
		</view>
		<view class="flex aic por fz28 color3">
			<text class="flex aic pl25r pr15r borderBox" style="width: 114px;justify-content: flex-start;height: 88rpx;">
				<text style="color: #e23;vertical-align: middle;">* </text>币种
			</text>
			<l-radio-group placement="row" current="1" bind:linchange="changeUnit">
				<l-radio key="1" class="color3 fz28">元</l-radio>
				<l-radio key="2" class="ml20 color3 fz28">美元</l-radio>
			</l-radio-group>
			<view class="poa bgColorF3" style="bottom: 0;right: 0;height: 2rpx;width: 730rpx;"></view>
		</view>
		<l-input label="汇率" placeholder="点击输入" required="{{true}}" type="number" value="{{exchange_rate}}"
			bind:lininput="changeRate" />
	</view>
	<view class="card">
		<view class="{{index===0?'por':'mt10 por'}}" wx:for="{{productList}}" wx:key="item" wx:for-index="index"
			wx:for-item="item">
			<view class="title flex jcsb">
				<view class="bold">产品描述{{index+1}}</view>
				<view wx:if="{{productList.length > 1}}" class="colorRed" data-index="{{index}}" bindtap="deleteProduct">删除该产品
				</view>
			</view>
			<view class="flex jcsb mb10 mt20">
				<view class="bgColorF5 w30">
					<view class="tc pt10">产品品类</view>
					<t-dropdown-menu z-index="2" class="productTypeIconHidden">
						<t-dropdown-item label="点击选择" optionsLayout="tree" options="{{productTypeList.options}}"
							value="{{item.type}}" bindchange="changeProductType" data-index="{{index}}" data-type='type' />
					</t-dropdown-menu>
				</view>
				<view class="bgColorF5 w30">
					<view class="tc pt10">目标价格</view>
					<input style="display: block;height: 48px;" value="{{item.client_target_price}}" class="pl10 pr10"
						type="number" bindinput="changeProductType" data-index="{{index}}" data-type="client_target_price"
						placeholder="输入价格" />
				</view>
				<view class="bgColorF5 w30">
					<view class="tc pt10">起订量</view>
					<input style="display: block;height: 48px;" value="{{item.start_order_number}}" class="pl10 pr10"
						type="number" bindinput="changeProductType" data-index="{{index}}" data-type="start_order_number"
						placeholder="输入起订量" />
				</view>
			</view>
			<l-textarea bind:lininput="changeProductType" indicator="{{true}}" border="{{false}}" data-index="{{index}}"
				value="{{item.desc}}" data-type="desc" maxlength="200" auto-height="{{true}}" placeholder="点击输入产品描述信息" />
			<van-uploader file-list="{{item.image_data}}" bind:before-read="beforeRead" data-index="{{index}}"
				bind:delete="deleteImage" bind:after-read="afterRead" />
		</view>
		<view class="color27A2 tc mt10" bindtap="addProduct">新增产品描述</view>
	</view>
	<view class="card">
		<view class="title">报价信息</view>
		<view>
			<van-tabs animated swipeable lazy-render ellipsis="{{false}}" swipe-threshold="{{4}}">
				<van-tab wx:for="{{productList}}" wx:key="item" wx:for-index="index" wx:for-item="item" title="产品 {{index+1}}"
					class="fz30">
					<!-- 原料 -->
					<view class="mt10" wx:for="{{item.material_data}}" wx:key="materialItem" wx:for-index="materialIndex"
						wx:for-item="materialItem">
						<view class="flex jcsb aic">
							<view class="flex fz25 aic w80">
								<view class="flex aic">
									<view class="bgColorOrange pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">原
									</view>
									<view class="ml10">{{materialItem.material_name}}</view>
								</view>
								<t-dropdown-menu z-index="2" class="yarnTypeListIconHidden">
									<t-dropdown-item label="点击变更" optionsLayout="tree" options="{{yarnTypeList.options}}"
										value="{{materialItem.material_id}}" bindchange="changeMaterial" data-index="{{index}}"
										data-materialIndex="{{materialIndex}}" />
								</t-dropdown-menu>
							</view>
							<view class="flex fz25">
								<view wx:if="{{materialIndex === 0}}" class="color27A2 pr5" bindtap="addProductType"
									data-index="{{index}}" data-type="material_data">添加</view>
								<view wx:if="{{materialIndex !== 0}}" class="colorE800 pl5" bindtap="deleteProductType"
									data-index="{{index}}" data-type="material_data" data-itemIndex="{{materialIndex}}">删除</view>
							</view>
						</view>
						<view class="flex flexWarp jcsb mb20">
							<view class="bgColorF5 w45 mt20 pl10 pt10 pb10">
								<view class="tc mb10">预计数量</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="weight"
									data-itemIndex="{{materialIndex}}" data-itemType="material_data" value="{{materialItem.weight}}"
									placeholder="预计数量" />
							</view>
							<view class="bgColorF5 w45 mt20 pl10 pt10 pb10">
								<view class="tc mb10">预计损耗</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="loss"
									data-itemIndex="{{materialIndex}}" data-itemType="material_data" maxlength="3"
									value="{{materialItem.loss}}" placeholder="预计损耗" />
							</view>
							<view class="bgColorF5 w45 mt20 pl10 pt10 pb10">
								<view class="tc mb10">单价</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="price"
									data-itemIndex="{{materialIndex}}" data-itemType="material_data" value="{{materialItem.price}}"
									placeholder="单价" />
							</view>
							<view class="bgColorF5 w45 mt20 pl10 pt10 pb10">
								<view class="tc mb10">小计</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="total_price"
									data-itemIndex="{{materialIndex}}" data-itemType="material_data" value="{{materialItem.total_price}}"
									placeholder="小计" />
							</view>
						</view>
					</view>
					<!-- 辅料 -->
					<view class="mt10" wx:for="{{item.assist_material_data}}" wx:key="assistItem" wx:for-index="assistIndex"
						wx:for-item="assistItem">
						<t-picker visible="{{assistItem.show}}" cancel-btn="取消" confirm-btn="确认" bindconfirm="chooseConfirm"
							data-index="{{index}}" data-itemIndex="{{assistIndex}}" data-type="assist_material_data"
							bindcancel="closeShow">
							<t-picker-item options="{{assistListChoose}}" />
						</t-picker>
						<view class="flex jcsb aic">
							<view class="flex fz25 aic w80">
								<view class="flex aic">
									<view class="bgColor03d0 pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">辅
									</view>
									<view class="ml10">{{assistItem.material_name}}</view>
								</view>
								<view class="ml20 color27A2" bindtap="showChoose" data-index="{{index}}"
									data-type="assist_material_data" data-itemIndex="{{assistIndex}}">点击变更</view>
							</view>
							<view class="flex fz25">
								<view wx:if="{{assistIndex === 0}}" class="color27A2 pr5" data-index="{{index}}"
									data-type="assist_material_data" bindtap="addProductType">添加</view>
								<view wx:if="{{assistIndex !== 0}}" class="colorE800 pl5" data-index="{{index}}"
									data-assistIndex="{{assistIndex}}" data-type="assist_material_data" data-itemIndex="{{assistIndex}}"
									bindtap="deleteProductType">
									删除</view>
							</view>
						</view>
						<view class="flex jcsb mb10 flexWarp">
							<view class="bgColorF5 mt20 w45 pl10 pb10 pt10">
								<view class="tc mb10">预计数量</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="number"
									data-itemIndex="{{assistIndex}}" data-itemType="assist_material_data" value="{{assistItem.number}}"
									placeholder="预计数量" />
							</view>
							<view class="bgColorF5 mt20 w45 pl10 pb10 pt10">
								<view class="tc mb10">预计损耗</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="loss"
									data-itemIndex="{{assistIndex}}" data-itemType="assist_material_data" value="{{assistItem.loss}}"
									placeholder="预计损耗" />
							</view>
							<view class="bgColorF5 mt20 w45 pl10 pb10 pt10">
								<view class="tc mb10">单价</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="price"
									data-itemIndex="{{assistIndex}}" data-itemType="assist_material_data" value="{{assistItem.price}}"
									placeholder="单价" />
							</view>
							<view class="bgColorF5 mt20 w45 pl10 pb10 pt10">
								<view class="tc mb10">小计</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="total_price"
									data-itemIndex="{{assistIndex}}" data-itemType="assist_material_data"
									value="{{assistItem.total_price}}" placeholder="小计" />
							</view>
						</view>
					</view>
					<!-- 织造 -->
					<view class="mt10" wx:for="{{item.weave_data}}" wx:key="itemWeaveData" wx:for-index="indexWeaveData"
						wx:for-item="itemWeaveData">
						<t-picker visible="{{itemWeaveData.show}}" cancel-btn="取消" confirm-btn="确认" bindconfirm="chooseConfirm"
							data-index="{{index}}" data-itemIndex="{{indexWeaveData}}" data-type="weave_data" bindcancel="closeShow">
							<t-picker-item options="{{weaveList}}" />
						</t-picker>
						<view class="flex jcsb aic">
							<view class="flex fz25 aic w80">
								<view class="flex aic">
									<view class="bgColor27A2 pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">织
									</view>
									<view class="ml20">{{itemWeaveData.name}}</view>
								</view>
								<view class="ml20 color27A2" bindtap="showChoose" data-index="{{index}}"
									data-itemIndex="{{indexWeaveData}}" data-type="weave_data">点击变更</view>
							</view>
							<view class="flex fz25">
								<view wx:if="{{indexWeaveData === 0}}" class="color27A2 pr5" data-index="{{index}}"
									data-type="weave_data" bindtap="addProductType">添加</view>
								<view wx:if="{{indexWeaveData !== 0}}" class="colorE800 pl5" data-index="{{index}}"
									data-indexWeaveData="{{indexWeaveData}}" data-type="weave_data" data-itemIndex="{{indexWeaveData}}"
									bindtap="deleteProductType">
									删除</view>
							</view>
						</view>
						<view class="flex jcsb mb10">
							<view class="bgColorF5 mt20 w60 pl10 pb10 pt10">
								<view class="tc mb10">费用说明</view>
								<input bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="desc"
									data-itemIndex="{{indexWeaveData}}" data-itemType="weave_data" value="{{itemWeaveData.desc}}"
									placeholder="费用说明" />
							</view>
							<view class="bgColorF5 mt20 w30 pl10 pb10 pt10">
								<view class="tc mb10">总价</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="total_price"
									data-itemIndex="{{indexWeaveData}}" data-itemType="weave_data" value="{{itemWeaveData.total_price}}"
									placeholder="总价" />
							</view>
						</view>
					</view>
					<!-- 半成品 -->
					<view class="mt10" wx:for="{{item.semi_product_data}}" wx:key="itemHalf" wx:for-index="indexHalf"
						wx:for-item="itemHalf">
						<t-picker visible="{{itemHalf.show}}" cancel-btn="取消" confirm-btn="确认" bindconfirm="chooseConfirm"
							data-index="{{index}}" data-itemIndex="{{indexHalf}}" data-type="semi_product_data"
							bindcancel="closeShow">
							<t-picker-item options="{{halfProcessList}}" />
						</t-picker>
						<view class="flex jcsb aic">
							<view class="flex fz25 aic w80">
								<view class="flex aic">
									<view class="bgColor27A2 pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">半
									</view>
									<view class="ml20">{{itemHalf.name}}</view>
								</view>
								<view class="ml20 color27A2" bindtap="showChoose" data-index="{{index}}" data-itemIndex="{{indexHalf}}"
									data-type="semi_product_data">点击变更</view>
							</view>
							<view class="flex fz25">
								<view wx:if="{{indexHalf === 0}}" class="color27A2 pr5" data-index="{{index}}"
									data-type="semi_product_data" bindtap="addProductType">添加</view>
								<view wx:if="{{indexHalf !== 0}}" class="colorE800 pl5" data-index="{{index}}"
									data-indexHalf="{{indexHalf}}" data-type="semi_product_data" data-itemIndex="{{indexHalf}}"
									bindtap="deleteProductType">
									删除</view>
							</view>
						</view>
						<view class="flex jcsb mb10">
							<view class="bgColorF5 mt20 w60 pl10 pb10 pt10">
								<view class="tc mb10">费用说明</view>
								<input bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="desc"
									data-itemIndex="{{indexHalf}}" data-itemType="semi_product_data" value="{{itemHalf.desc}}"
									placeholder="费用说明" />
							</view>
							<view class="bgColorF5 mt20 w30 pl10 pb10 pt10">
								<view class="tc mb10">总价</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="total_price"
									data-itemIndex="{{indexHalf}}" data-itemType="semi_product_data" value="{{itemHalf.total_price}}"
									placeholder="总价" />
							</view>
						</view>
					</view>
					<!-- 成品 -->
					<view class="mt10" wx:for="{{item.production_data}}" wx:key="itemPD" wx:for-index="indexPD"
						wx:for-item="itemPD">
						<t-picker visible="{{itemPD.show}}" cancel-btn="取消" confirm-btn="确认" bindconfirm="chooseConfirm"
							data-index="{{index}}" data-itemIndex="{{indexPD}}" data-type="production_data" bindcancel="closeShow">
							<t-picker-item options="{{processList}}" />
						</t-picker>
						<view class="flex jcsb aic">
							<view class="flex fz25 aic w80">
								<view class="flex aic">
									<view class="bgColor27A2 pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">成
									</view>
									<view class="ml20">{{itemPD.name}}</view>
								</view>
								<view class="ml20 color27A2" bindtap="showChoose" data-index="{{index}}" data-itemIndex="{{indexPD}}"
									data-type="production_data">点击变更</view>
							</view>
							<view class="flex fz25">
								<view wx:if="{{indexPD === 0}}" class="color27A2 pr5" data-index="{{index}}" data-type="production_data"
									bindtap="addProductType">添加</view>
								<view wx:if="{{indexPD !== 0}}" class="colorE800 pl5" data-index="{{index}}" data-indexPD="{{indexPD}}"
									data-type="production_data" data-itemIndex="{{indexPD}}" bindtap="deleteProductType">
									删除</view>
							</view>
						</view>
						<view class="flex jcsb mb10">
							<view class="bgColorF5 mt20 w60 pl10 pb10 pt10">
								<view class="tc mb10">费用说明</view>
								<input bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="desc"
									data-itemIndex="{{indexPD}}" data-itemType="production_data" value="{{itemPD.desc}}"
									placeholder="费用说明" />
							</view>
							<view class="bgColorF5 mt20 w30 pl10 pb10 pt10">
								<view class="tc mb10">总价</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="total_price"
									data-itemIndex="{{indexPD}}" data-itemType="production_data" value="{{itemPD.total_price}}"
									placeholder="总价" />
							</view>
						</view>
					</view>
					<!-- 包装 -->
					<view class="mt10" wx:for="{{item.pack_material_data}}" wx:key="itemPack" wx:for-index="indexPack"
						wx:for-item="itemPack">
						<t-picker visible="{{itemPack.show}}" cancel-btn="取消" confirm-btn="确认" bindconfirm="chooseConfirm"
							data-index="{{index}}" data-itemIndex="{{indexPack}}" data-type="pack_material_data"
							bindcancel="closeShow">
							<t-picker-item options="{{packingList}}" />
						</t-picker>
						<view class="flex jcsb aic">
							<view class="flex fz25 aic w80">
								<view class="flex aic">
									<view class="bgColor27A2 pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">包
									</view>
									<view class="ml20">{{itemPack.material_name}}</view>
								</view>
								<view class="ml20 color27A2" bindtap="showChoose" data-index="{{index}}" data-itemIndex="{{indexPack}}"
									data-type="pack_material_data">点击变更</view>
							</view>
							<view class="flex fz25">
								<view wx:if="{{indexPack === 0}}" class="color27A2 pr5" data-index="{{index}}"
									data-type="pack_material_data" bindtap="addProductType">添加</view>
								<view wx:if="{{indexPack !== 0}}" class="colorE800 pl5" data-index="{{index}}"
									data-indexPack="{{indexPack}}" data-type="pack_material_data" data-itemIndex="{{indexPack}}"
									bindtap="deleteProductType">
									删除</view>
							</view>
						</view>
						<view class="flex jcsb mb10">
							<view class="bgColorF5 mt20 w60 pl10 pb10 pt10">
								<view class="tc mb10">费用说明</view>
								<input bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="desc"
									data-itemIndex="{{indexPack}}" data-itemType="pack_material_data" value="{{itemPack.desc}}"
									placeholder="费用说明" />
							</view>
							<view class="bgColorF5 mt20 w30 pl10 pb10 pt10">
								<view class="tc mb10">总价</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="total_price"
									data-itemIndex="{{indexPack}}" data-itemType="pack_material_data" value="{{itemPack.total_price}}"
									placeholder="总价" />
							</view>
						</view>
					</view>
					<!-- 其它费用 -->
					<view class="mt10" wx:for="{{item.other_fee_data}}" wx:key="itemOtherFeeData" wx:for-index="indexOtherFeeData"
						wx:for-item="itemOtherFeeData">
						<view class="flex jcsb aic">
							<view class="flex fz25 aic w80">
								<view class="flex aic">
									<view class="bgColor27A2 pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">其
									</view>
								</view>
							</view>
							<view class="flex fz25">
								<view wx:if="{{indexOtherFeeData === 0}}" class="color27A2 pr5" data-index="{{index}}"
									data-type="other_fee_data" bindtap="addProductType">添加</view>
								<view wx:if="{{indexOtherFeeData !== 0}}" class="colorE800 pl5" data-index="{{index}}"
									data-indexOtherFeeData="{{indexOtherFeeData}}" data-type="other_fee_data"
									data-itemIndex="{{indexOtherFeeData}}" bindtap="deleteProductType">
									删除</view>
							</view>
						</view>
						<view class="flex jcsb mb10">
							<view class="bgColorF5 mt20 w28 pl10 pb10 pt10">
								<view class="tc mb10">其它费用</view>
								<input bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="name"
									data-itemIndex="{{indexOtherFeeData}}" data-itemType="other_fee_data"
									value="{{itemOtherFeeData.name}}" placeholder="其它费用" />
							</view>
							<view class="bgColorF5 mt20 w28 pl10 pb10 pt10">
								<view class="tc mb10">费用说明</view>
								<input bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="desc"
									data-itemIndex="{{indexOtherFeeData}}" data-itemType="other_fee_data"
									value="{{itemOtherFeeData.desc}}" placeholder="费用说明" />
							</view>
							<view class="bgColorF5 mt20 w28 pl10 pb10 pt10">
								<view class="tc mb10">总价</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="total_price"
									data-itemIndex="{{indexOtherFeeData}}" data-itemType="other_fee_data"
									value="{{itemOtherFeeData.total_price}}" placeholder="总价" />
							</view>
						</view>
					</view>
					<!-- 非生产型费用 -->
					<view class="mt10" wx:for="{{item.no_production_fee_data}}" wx:key="itemNoProductFeeData"
						wx:for-index="indexNoProductFeeData" wx:for-item="itemNoProductFeeData">
						<view class="flex jcsb aic">
							<view class="flex fz25 aic w80">
								<view class="flex aic">
									<view class="bgColor27A2 pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">非
									</view>
								</view>
							</view>
							<view class="flex fz25">
								<view wx:if="{{indexNoProductFeeData === 0}}" class="color27A2 pr5" data-index="{{index}}"
									data-type="no_production_fee_data" bindtap="addProductType">添加</view>
								<view wx:if="{{indexNoProductFeeData !== 0}}" class="colorE800 pl5" data-index="{{index}}"
									data-indexNoProductFeeData="{{indexNoProductFeeData}}" data-type="no_production_fee_data"
									data-itemIndex="{{indexNoProductFeeData}}" bindtap="deleteProductType">
									删除</view>
							</view>
						</view>
						<view class="flex jcsb mb10">
							<view class="bgColorF5 mt20 w28 pl10 pb10 pt10">
								<view class="tc mb10">非生产型费用</view>
								<input bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="name"
									data-itemIndex="{{indexNoProductFeeData}}" data-itemType="no_production_fee_data"
									value="{{itemNoProductFeeData.name}}" placeholder="非生产型费用" />
							</view>
							<view class="bgColorF5 mt20 w28 pl10 pb10 pt10">
								<view class="tc mb10">费用说明</view>
								<input bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="desc"
									data-itemIndex="{{indexNoProductFeeData}}" data-itemType="no_production_fee_data"
									value="{{itemNoProductFeeData.desc}}" placeholder="费用说明" />
							</view>
							<view class="bgColorF5 mt20 w28 pl10 pb10 pt10">
								<view class="tc mb10">总价</view>
								<input type="number" bindinput="changeProductTypeDetail" data-index="{{index}}" data-type="total_price"
									data-itemIndex="{{indexNoProductFeeData}}" data-itemType="no_production_fee_data"
									value="{{itemNoProductFeeData.total_price}}" placeholder="总价" />
							</view>
						</view>
					</view>
					<!-- 运输费用 -->
					<view class="bgColor27A2 pl8 pr12 pt5 pb5 colorWhite" style="border-radius: 500px;width:13px;">运</view>
					<view class="flex jcsb mb10">
						<view class="bgColorF5 mt20 w45 pl10 pb10 pt10">
							<view class="tc mb10">运输费用</view>
							<input type="number" bindinput="changeProductType" data-type="transport_fee" data-index="{{index}}"
								value="{{item.transport_fee}}" placeholder="运输费用" />
						</view>
						<view class="bgColorF5 mt20 w45 pl10 pb10 pt10">
							<view class="tc mb10">费用说明</view>
							<input bindinput="changeProductType" data-type="transport_fee_desc" data-index="{{index}}"
								value="{{item.transport_fee_desc}}" placeholder="费用说明" />
						</view>
					</view>
				</van-tab>
			</van-tabs>
		</view>
	</view>
	<view class="card">
		<view class="title">订单费用</view>
		<view class="flex jcsb mb10 flexWarp">
			<view class="bgColorF5 w48 mt10 pt10 pb10 tc">
				<view>报价产品数量</view>
				<view class="mt10">{{productList.length}} 款</view>
			</view>
			<view class="bgColorF5 w48 mt10 pt10 pb10 tc">
				<view>产品成本价合计</view>
				<view class="mt10">{{total_cost_price}} 元</view>
			</view>
			<view class="bgColorF5 w45 mt10 pt10 pb10 pl10">
				<view class="tc">客户佣金百分比</view>
				<input type="number" style="display: block;height: 48px;" bindinput="inputCommission" placeholder="客户佣金百分比" />
			</view>
			<view class="bgColorF5 w48 mt10 pt10 pb10 tc">
				<view>佣金费用</view>
				<view class="mt10">{{commission_price}} 元</view>
			</view>
			<view class="bgColorF5 w45 mt10 pt10 pb10 pl10">
				<view class="tc">预计税率</view>
				<input type="number" style="display: block;height: 48px;" bindinput="inputRateTaxation" placeholder="预计税率" />
			</view>
			<view class="bgColorF5 w48 mt10 pt10 pb10 tc">
				<view>税费</view>
				<view class="mt10">{{rate_price}} 元</view>
			</view>
			<view class="bgColorF5 w45 mt10 pt10 pb10 pl10">
				<view class="tc">预计利润百分比</view>
				<input type="number" style="display: block;height: 48px;" bindinput="inputProfitPercentage"
					placeholder="预计利润百分比" />
			</view>
			<view class="bgColorF5 w48 mt10 pt10 pb10 tc">
				<view>利润</view>
				<view class="mt10">{{profit_price}} 元</view>
			</view>
			<view class="bgColorF5 w45 mt10 pt10 pb10 pl10">
				<view class="tc">其它备注与说明</view>
				<input style="display: block;height: 48px;" placeholder="其它备注与说明" />
			</view>
			<view class="bgColorF5 w45 mt10 pt10 pb10 pl10">
				<view class="tc">最终客户报价</view>
				<input type="number" style="display: block;height: 48px;" placeholder="最终客户报价" />
			</view>
		</view>
	</view>
	<view class="vh12"></view>
	<view wx:if="{{isLogin}}">
		<view class="mt20 submit" style="position: fixed;bottom:5%;left:50%;transform: translateX(-50%);z-index:2">
			<view class="flex jcsa">
				<van-button type="primary" size="small" bind:click="submitAllInfo" data-isCaogao="{{true}}">保存为草稿</van-button>
				<van-button style="margin-left: 20px;" type="primary" size="small" bind:click="submitAllInfo"
					data-isCaogao="{{false}}">提交</van-button>
			</view>
		</view>
	</view>
	<l-message content="{{content}}" />
</view>