/* t-image 组件样式 */
t-image {
  border-radius: 0;
  overflow: hidden;
}

.image{
  width: 200rpx;
  height: 150rpx;
}

.t-image__mask {
  background-color: #f2f2f2;
}

.radius-l .t-image {
  border-radius: 16rpx;
}

.radius-m .t-image {
  border-radius: 8rpx;
}

.radius-s .t-image {
  border-radius: 4rpx;
}

.radius-round .t-image {
  border-radius: 999px;
}

.size-xl .t-image {
  width: 240rpx;
  height: 240rpx;
}

.size-l .t-image {
  width: 144rpx;
  height: 144rpx;
}

.size-m .t-image {
  width: 112rpx;
  height: 112rpx;
}

.size-s .t-image {
  width: 96rpx;
  height: 96rpx;
}

.size-xs .t-image {
  width: 64rpx;
  height: 64rpx;
}

.size-xss .t-image {
  width: 48rpx;
  height: 48rpx;
}

.size-xsss .t-image {
  width: 38rpx;
  height: 38rpx;
}








/* t-button */
.submit .t-button {
  background-image: linear-gradient( #52C0FF, #008AD8);
  color: #ffffff!important;
  border: none;
  width: 80vw;
  border-top-left-radius: 37rpx;
  border-top-right-radius: 37rpx;
  border-bottom-left-radius: 37rpx;
  border-bottom-right-radius: 37rpx;
  margin: 0 auto;
}
.half .t-button {
  color: #ffffff!important;
  border: none;
  width: 40%;
  border-top-left-radius: 37rpx;
  border-top-right-radius: 37rpx;
  border-bottom-left-radius: 37rpx;
  border-bottom-right-radius: 37rpx;
  margin: 0 auto;
}
.half .t-button.submit {
  background-image: linear-gradient( #52C0FF, #008AD8);
}
.half .t-button.refuse {
  background-image: linear-gradient( #c7c7c7, #797979);
}








/* t-tabs */
.t-tabs__scroll .t-is-active{
