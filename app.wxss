.card {
	overflow: hidden;
	box-shadow: 0px 5px 20px #00000014;
	position: relative;
	box-sizing: border-box;
	background-color: white;
	border-radius: 16rpx;
	width: 90vw;
	padding-bottom: 10px;
	padding-right: 10px;
	padding-left: 10px;
	padding-top: 10px;
	margin-right: 20px;
	margin-left: 20px;
	margin-top: 10px;
	justify-content: space-between;
	flex-wrap: wrap;
	color: black;
}

.card>.title {
